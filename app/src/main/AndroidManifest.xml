<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wrj.film">

    <application
        android:name=".AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ui.activity.RegisteredActivity" />
        <!-- <activity android:name=".view.ui.activity.MainActivity"/> -->
        <activity android:name=".view.ui.activity.FilmDetailActivity" />
        <activity android:name=".view.ui.activity.FilmBuyActivity" />
        <activity android:name=".view.ui.activity.SelectTableActivity"></activity>
    </application>

</manifest>